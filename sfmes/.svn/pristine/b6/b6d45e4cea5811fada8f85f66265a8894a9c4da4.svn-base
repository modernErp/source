						currObj.ClearSelect(currTr);
						if(cellInfo.bCode) currObj.DisableCodeButton(cell);
						else if(cellInfo.bSpin) currObj.DisableSpinButton(cell);
						return;
					}
				}
				else
				{
					var id = currObj.GetCellId(cell);
