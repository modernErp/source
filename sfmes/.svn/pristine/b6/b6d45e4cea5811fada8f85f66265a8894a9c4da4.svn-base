		var ___bAll = true;
		var ___bTrue = true;
		var ___bToggle = false;
		var ___sWhere = '';
		var ___nRow = '';
		if(___arr[0].toLowerCase() == 'false') ___bTrue = false;
		else if(___arr[0].toLowerCase() == 'toggle') ___bToggle = true;
		if(___arr.length>1)
		{
			if(___arr[1].toLowerCase() == 'where')
			{
				for(var i=1; i<___arr.length; i++)
				{
