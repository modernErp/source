		}
		var bReturn = false;
		var fType=obj.getAttribute('accept');
		if(fType && WebcreaInfo.bFileTypeCheck)
		{
			var arr=split(fType,',');
			for(var i=0;i<obj.files.length; i++)
			{
				var fType=obj.files[i].type;
				var bOK = true;
				for(var j=0; j<arr.length; j++)
				{
					var aType = trim(arr[j]);
					if(aType.indexOf('/*')>0)
					{
						aType=split(aType,'/')[0];
						fType=split(fType,'/')[0];
					}
					if(aType == fType)
					{
						bOK = true;
						break;
					}
					else bOK = false;
				}
				if(!bOK)
				{
					bReturn = true;
					break;
