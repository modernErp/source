		this.objFDivPos = Webcrea.FindNode(this.objDivPage, 'id', this.divPos + 'F');
		this.objTDivPos = Webcrea.FindNode(this.objDivPage, 'id', this.divPos + 'T');
		this.objDivPos = Webcrea.FindNode(this.objDivPage, 'id', this.divPos);
