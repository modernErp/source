					for(var i=0; i<childNodes.length; i++)
					{
						var cellChild = childNodes[i];
						if(cellChild.nodeName != 'INPUT')
						{
							if(cellChild.nodeName == 'DIV') SetChildAttr(cellChild);
							continue;
						}
						if(cellId && cellId != '')
						{
							var sCellId = currObj.GetCellId(cellChild);
