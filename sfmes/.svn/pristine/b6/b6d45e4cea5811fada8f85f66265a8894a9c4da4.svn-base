	},
	SetBWZDisplay : function()
	{
		var currNode = arguments[0];
		var bOut = arguments[1];
		if(Webcrea.IsEditTd(currNode)) return;
		if(Webcrea.IsMultilineMainDiv(currNode)) return;
		var tdNode = Webcrea.FindParentNode(currNode, 'name', 'TD');
