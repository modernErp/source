	{
		let nGDivLeft = 0;
		if(obj.objGDivPos) nGDivLeft = Webcrea.GetLeft(obj.objGDivPos, bOffset);
		return Webcrea.GetLeft(obj.objDivPos, bOffset) + nGDivLeft;
