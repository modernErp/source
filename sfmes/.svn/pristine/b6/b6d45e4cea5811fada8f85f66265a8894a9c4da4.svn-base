			{
				objBottom.layoutOpt.bVerting = true;
				objBottom.OnResizeVert(nExpandOrg);
				var nObjTop = (objBottom.objGDivPos) ? Webcrea.GetTop(objBottom.objGDivPos) : Webcrea.GetTop(objBottom.objDivPos);
