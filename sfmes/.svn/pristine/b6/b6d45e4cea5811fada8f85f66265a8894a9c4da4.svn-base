			{
				var dataRowNo = trNode.getAttribute('rowno');
				if(dataRowNo == currObj.objSelInfo.dataRowNo)
				{
					var nRow = currObj.GetRowNo(dataRowNo);
					currObj.SetCurrRowInfo(nRow, dataRowNo);
				}
			}
		}
	},
