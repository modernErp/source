					{
						if(currObj.IsMulti(cell.id))
						{
							var objNext = cell.nextElementSibling;
							while(objNext)
							{
								if(objNext && objNext.nodeName == 'INPUT') objNext.disabled = !b;
								objNext = objNext.nextElementSibling;
							}
