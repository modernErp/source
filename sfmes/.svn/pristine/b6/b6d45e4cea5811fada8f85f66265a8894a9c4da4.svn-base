			var param1 = param.substr(0, n);
			var param2 = param.substr(n+sIndex.length);
			var n1 = param2.indexOf('&');
			if(n1>0)
			{
				sInfo = param2.substr(0, n1);
				param2 = param2.substr(n1);
			}
