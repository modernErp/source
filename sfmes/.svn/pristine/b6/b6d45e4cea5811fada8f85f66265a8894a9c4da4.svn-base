						}
						else
						{
							var id = currObj.GetCellId(cellSubChild);
							var cellInfo = currObj.GetCellInfo(id);
							if(cellInfo && (cellInfo.bReadOnly || !cellInfo.bEnable)) return;
							cellSubChild.disabled = !b;
						}
