				arr[seq].style.width = calWidth + 'px';
				arr[seq].setAttribute('display', '1');
				if(!currObj.layoutOpt.bHorz) arr[seq].style.visibility='';
			}
			else
			{
				var cnt=0;
				var calWidth=0;
