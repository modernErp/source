			}
			var colGroup = Webcrea.FindNode(currObj.objDivPos, 'name', 'COLGROUP');
			Webcrea.FindNodeAll(colGroup, 'name', 'COL', arr);

			if(arr[seq].getAttribute('display') == '1') return;
