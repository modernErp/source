	CallFunc : function()
	{
		if(!this.IsHtml()) return;
		var ___param = {};
		___param.func = arguments[0];
		___param.param = arguments[1];
		if(Webcrea.IsEmpty(___param.func)) return;
		var ifrm = Webcrea.FindNode(this.objDivPos, 'name', 'IFRAME');
