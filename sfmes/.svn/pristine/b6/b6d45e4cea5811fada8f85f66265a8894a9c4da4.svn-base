				Webcrea.objMoveLayer.bCaptionMove = bCaptionMove;
				var nCurrLeft = Webcrea.GetLeft(moveDiv);
				var nCurrTop = Webcrea.GetTop(moveDiv);
				var nMouseLeft = Webcrea.GetMousePos(0);
