					{
						arrGroupItem[arrGroupItem.length] = arrSubParam[j];
					}
					else nMultiplyValue = Webcrea.NumberReDef(arrSubParam[j]);
				}
			}
		}
		if(bPercent) nMultiplyValue = 100;
