				if(bCurrNode) continue;
				var bMoveNode=false;
				for(var k=0; k<arrMoveTdNode.length; k++)
				{
					if(colNode == arrMoveTdNode[k])
					{
						bMoveNode=true;
						break;
					}
				}
				if(bMoveNode)
				{
					if(bAppend) continue;
					if(bAfter)
					{
						for(var k=0; k<arrMoveTdNode.length; k++)
						{
							var cloneNode = arrMoveTdNode[k].cloneNode(true);
