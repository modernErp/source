					var nLineHeight = Number(Extract(node.style.lineHeight, '[0-9]'));
					if(nLineHeight>0 && Webcrea.IE())
					{
						if(bBold)
						{
							nLineHeight -= 0;
							node.style.verticalAlign = 'middle';
