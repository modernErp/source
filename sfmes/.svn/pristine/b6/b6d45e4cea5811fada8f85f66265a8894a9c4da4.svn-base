				nodeParent = nodeParent.parentNode;
			}
			if(!findNode) return;
			cell = findNode;
		}
		if(!Webcrea.IsEmpty(image))
		{
			cell.style.backgroundImage = "url('" + Webcrea.GetImageLinkPath() + "/" + image + "')";
			cell.style.backgroundRepeat = 'no-repeat';
			cell.style.backgroundSize = '100% 100%';
