	this.CreateContextMenu = function(objContext, currObj)
	{
		var obj = null;
		if(!objContext) return;
		var nLeft = Webcrea.GetMousePos(0);
		var nTop = Webcrea.GetMousePos(1);
		var id = objContext.id;
