				else width = this.width;
				var clickEvent = this.objName + '.ToggleAccordion(this);';
				tDiv.setAttribute('onclick', 'javascript:' + clickEvent);
				var nHMain = this.nHMain;
				if(this.align == 1 && nHMain==0 && this.nHPos>0)
