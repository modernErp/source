						}
						if(bRight)
						{
							if(subImgLast && subImgLast.tagName == 'IMG')
							{
								if(!Webcrea.IsEmpty(bulletPath)) subImgLast.src = bulletPath;
								else
								{
									bulletWidth = bulletWidthLeft;
