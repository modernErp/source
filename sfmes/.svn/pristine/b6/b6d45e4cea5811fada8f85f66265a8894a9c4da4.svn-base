						var nCheckRow = this.GetRowNo(i);
						this.SetValueByData('_SELECT_', value, nCheckRow);
					}
				}
				else
				{
					var value = this.arrMulti[___nRow];
					var rowNo = this.GetRowNo(___nRow);
					this.SetValueByData('_SELECT_', value, rowNo);
