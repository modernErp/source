				{
					bSqlExist = false;
					break;
				}
				if(i>0) sSqlIndex += spChar;
				sSqlIndex += sInfo;

				param = GetInfo(param, '&paramIndex=');
				if(i>0) sParamIndex += spChar;
				sParamIndex += sInfo;

				param = GetInfo(param, '&urlIndex=');
				if(i>0) sUrlIndex += spChar;
				sUrlIndex += sInfo;

				param = GetInfo(param, '&sqlParamInfo=');
				if(i>0) sSqlParamInfo += spChar;
				sSqlParamInfo += sInfo;
