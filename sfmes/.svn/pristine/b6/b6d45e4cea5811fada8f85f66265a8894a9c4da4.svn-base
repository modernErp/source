				}
				if(!Webcrea.IsEmpty(sAgent) && !Webcrea.IsEmpty(sUrl))
				{
					bAgentError = true;
					break;
				}
				n = param.indexOf('&sql=');
				param = GetInfo(param, '&sql=');
				if(Webcrea.IsEmpty(sInfo))
				{
					bSqlExist = false;
					break;
				}
				if(i>0) sSql += spChar;
