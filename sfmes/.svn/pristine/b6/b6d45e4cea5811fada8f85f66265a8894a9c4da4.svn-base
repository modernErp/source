			else if(cell.nodeName == 'LABEL')
			{
				var cellSubChild = cell.firstChild;
				while(cellSubChild)
				{
					if(cellSubChild.nodeName == 'INPUT' && (cellSubChild.type == 'checkbox' || cellSubChild.type == 'radio'))
					{
						if(cellId && cellId != '')
