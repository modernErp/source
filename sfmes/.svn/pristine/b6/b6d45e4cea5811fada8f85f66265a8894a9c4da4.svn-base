		this.arrStackEvent[this.arrStackEvent.length] = 'ontitle';
		if(typeof window[this.divPage][this.divPosOrg].OnTITLE != 'undefined') window[this.divPage][this.divPosOrg].OnTITLE();
		this.arrStackEvent.removeByValue('ontitle');
