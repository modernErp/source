		else if(opt.toLowerCase() == 'noempty') bNoEmpty=true;
		else if(opt.toLowerCase() == 'hidden') bHidden=true;
		else if(opt.toLowerCase() == 'number') bNumber=true;
		else if(opt.toLowerCase() == 'array') bArray=true;
	}
	var arrData = [];
