		Webcrea.MoveProgress();
		if(sMsg)
		{
			if(totalSec>0) this.aniProgress = setInterval(function(){ Webcrea.StopProgress();}, totalSec*1000);
		}
		else if(nMode != 2) this.aniProgress = setInterval(function(){ Webcrea.RunProgress();}, time);
